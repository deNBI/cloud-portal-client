{
  "extends": [
    "config:base"
  ],
  "baseBranches": ["dev"],
  "reviewers": ["team:portal-dev"],
  "labels": ["dependencies"],
  "packageRules": [
    {
      "matchDatasources": ["docker"],
      "labels": ["docker"]
    }
  ],
  "assignees": ["team:portal-dev"],
  "branchPrefix": "deps/",
